# API Configuration
# Copy this file to .env and update with your actual values

# Base URL for the API (without trailing slash)
EXPO_PUBLIC_API_BASE_URL=http://localhost

# API Key for authentication (if required by your backend)
EXPO_PUBLIC_API_KEY=your_api_key_here

# Pushy.me API Key for Push Notifications
# Get your API key from https://dashboard.pushy.me/
EXPO_PUBLIC_PUSHY_API_KEY=your_pushy_api_key_here
